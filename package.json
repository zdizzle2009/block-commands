{
	"name": "block-commands",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"astro": "astro",
		"build": "astro check && astro build",
		"check": "astro check && pnpm run check:format && pnpm run check:lint && pnpm run check:types",
		"check:format": "pnpm prettier . --cache --check",
		"check:lint": "TIMING=1 eslint \"src/**/*.{js,ts,jsx,tsx,astro}\" --max-warnings 0 --report-unused-disable-directives --cache --cache-location ./node_modules/.cache/eslint",
		"check:types": "tsc --noEmit",
		"dev": "astro dev",
		"fix": "pnpm fix:lint && pnpm fix:format",
		"fix:format": "pnpm prettier . --cache --write",
		"fix:lint": "TIMING=1 eslint \"src/**/*.{js,ts,jsx,tsx,astro}\" --max-warnings 0 --report-unused-disable-directives --cache --cache-location ./node_modules/.cache/eslint --fix",
		"start": "npx serve ./dist"
	},
	"dependencies": {
		"@astrojs/check": "^0.9.4",
		"@astrojs/markdoc": "^0.12.4",
		"@astrojs/netlify": "^6.0.1",
		"@astrojs/react": "^4.1.2",
		"@astrojs/rss": "^4.0.10",
		"@astrojs/sitemap": "^3.2.1",
		"@astrojs/tailwind": "^5.1.4",
		"@fontsource/press-start-2p": "^5.1.0",
		"@keystatic/astro": "^5.0.3",
		"@keystatic/core": "^0.5.42",
		"astro": "^5.1.1",
		"date-fns": "^4.1.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"tailwindcss": "^4.0.0",
		"tiny-invariant": "^1.3.3"
	},
	"devDependencies": {
		"@epic-web/config": "^1.16.4",
		"@eslint/compat": "^1.2.4",
		"@eslint/js": "^9.17.0",
		"@ianvs/prettier-plugin-sort-imports": "^4.4.0",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.2",
		"@types/react": "^19.0.2",
		"@types/react-dom": "^19.0.2",
		"eslint": "^9.17.0",
		"eslint-plugin-react": "^7.37.2",
		"globals": "^15.14.0",
		"prettier": "^3.4.2",
		"prettier-plugin-astro": "^0.14.1",
		"prettier-plugin-curly": "^0.3.1",
		"prettier-plugin-packagejson": "^2.5.6",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"react-compiler-runtime": "19.0.0-beta-201e55d-20241215",
		"typescript": "^5.7.2"
	},
	"packageManager": "pnpm@9.15.1+sha512.1acb565e6193efbebda772702950469150cf12bcc764262e7587e71d19dc98a423dff9536e57ea44c49bdf790ff694e83c27be5faa23d67e0c033b583be4bfcf",
	"engines": {
		"node": ">=22"
	}
}
