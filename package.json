{
	"name": "block-commands",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"astro": "astro",
		"build": "astro check && astro build",
		"check": "astro check && pnpm run check:format && pnpm run check:lint && pnpm run check:types",
		"check:format": "pnpm prettier . --cache --check",
		"check:lint": "TIMING=1 eslint \"src/**/*.{js,ts,jsx,tsx,astro}\" --max-warnings 0 --report-unused-disable-directives --cache --cache-location ./node_modules/.cache/eslint",
		"check:types": "tsc --noEmit",
		"dev": "astro dev",
		"fix": "pnpm fix:lint && pnpm fix:format",
		"fix:format": "pnpm prettier . --cache --write",
		"fix:lint": "TIMING=1 eslint \"src/**/*.{js,ts,jsx,tsx,astro}\" --max-warnings 0 --report-unused-disable-directives --cache --cache-location ./node_modules/.cache/eslint --fix",
		"start": "npx serve ./dist"
	},
	"dependencies": {
		"@astrojs/check": "^0.9.4",
		"@astrojs/markdoc": "^0.12.11",
		"@astrojs/netlify": "^6.5.10",
		"@astrojs/react": "^4.3.1",
		"@astrojs/rss": "^4.0.12",
		"@astrojs/sitemap": "^3.6.0",
		"@astrojs/tailwind": "^6.0.2",
		"@fontsource/press-start-2p": "^5.2.7",
		"@keystatic/astro": "^5.0.6",
		"@keystatic/core": "^0.5.48",
		"astro": "^5.13.8",
		"date-fns": "^4.1.0",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"tailwindcss": "^4.1.13",
		"tiny-invariant": "^1.3.3"
	},
	"devDependencies": {
		"@epic-web/config": "^1.21.3",
		"@eslint/compat": "^2.0.0",
		"@eslint/js": "^9.35.0",
		"@ianvs/prettier-plugin-sort-imports": "^4.7.0",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.13",
		"@types/react": "^19.1.13",
		"@types/react-dom": "^19.1.9",
		"eslint": "^9.35.0",
		"eslint-plugin-react": "^7.37.5",
		"globals": "^16.4.0",
		"prettier": "^3.6.2",
		"prettier-plugin-astro": "^0.14.1",
		"prettier-plugin-curly": "^0.4.0",
		"prettier-plugin-packagejson": "^2.5.19",
		"prettier-plugin-tailwindcss": "^0.7.0",
		"react-compiler-runtime": "19.0.0-beta-ebf51a3-20250411",
		"typescript": "^5.9.2"
	},
	"packageManager": "pnpm@10.17.0+sha512.fce8a3dd29a4ed2ec566fb53efbb04d8c44a0f05bc6f24a73046910fb9c3ce7afa35a0980500668fa3573345bd644644fa98338fa168235c80f4aa17aa17fbef",
	"engines": {
		"node": ">=22"
	}
}
